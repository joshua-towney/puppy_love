<h1>Create Your Account</h1>

<form action="/login" autocomplete="off" method="post">

    <label>User Name</label>
    <input name="user_name" type="text" onkeypress=" return (event.keyCode !== 39) && (event.keyCode !== 34)" required>

    <label>Email</label>
    <input name="email" type="text" onkeypress="return (event.keyCode !== 39) && (event.keyCode !== 34)" required>

    <label>Password</label>
    <input name="password" type="password" onkeypress="return (event.keyCode !== 39) && (event.keyCode !== 34)" required>

    <button>Create Account</button>

</form>